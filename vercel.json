{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/login",
            "dest": "/login.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "cleanUrls": true
} 
