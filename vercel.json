{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/trending-words/(.*)",
            "dest": "/trending-words/$1.html"
        },
        {
            "src": "/forum",
            "dest": "/forum.html"
        },
        {
            "src": "/about",
            "dest": "/about.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 
